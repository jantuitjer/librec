@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dbc: <http://dbpedia.org/resource/Category:> . 
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://dublincore.org/documents/2012/06/14/dcmi-terms/> .
@prefix schema: <https://schema.org/place> .
@prefix jt: <http://schema.example.org/> .
@prefix xsd: <https://www.w3.org/2011/rdf-wg/wiki/XSD_Datatypes> .
@prefix ex:   <http://example.org/>.


# definition of UserFilmRating class
jt:UserFilmRating owl:equivalentClass [
    owl:onProperty jt:hasRated ;
    owl:onProperty jt:withRating ;
    owl:onProperty jt:atTime ;
	owl:onProperty jt:wasGivenBy;
].
# definition of UserGenreInterest class
jt:UserGenreInterest owl:equivalentClass [
    owl:onProperty jt:interestWeight;
].

#definition of wasGivenBy property
jt:wasGivenBy rdfs:domain jt:UserFilmRating;
	rdfs:range foaf:Person.

jt:hasGiven owl:inverseOf jt:wasGivenBy.

jt:interestWeight rdfs:domain jt:UserGenreInterest;
	rdfs:range xsd:integer.
	
# definition of withRating property
jt:withRating	a rdf:Property;
	rdfs:domain jt:UserFilmRating;
	rdfs:range xsd:double.
	
# definition of atTime property
jt:atTime rdfs:domain jt:UserFilmRating;
	rdfs:range xsd:integer.

# definition of hasRated property
jt:hasRated rdfs:domain jt:UserFilmRating;
	rdfs:range dbo:Film.

#definition of total rating literal
jt:totalRatings a rdfs:Literal;
	rdfs:range xsd:nonNegativeInteger.

#definition of number of ratings property 
jt:numberOfRatings a rdf:Property;
	rdfs:domain dbo:Film;
	rdfs:range jt:totalRatings.

#definition of avg rating literal
jt:avgOfRatings a rdfs:Literal;
	rdfs:range xsd:double.

#definition of avg of ratings property 
jt:avgRating rdfs:domain dbo:Film;
	rdfs:range jt:avgOfRatings.

#defintion property favoriteGenre
jt:favoriteGenre rdfs:domain foaf:Person;
	rdfs:range dbo:Genre.
	
#defintion property of genre
jt:ofGenre rdfs:domain dbo:Film;
	rdfs:range dbo:Genre.

#defintion jt:highestRating
jt:highestRating rdfs:domain foaf:Person;
	rdfs:range xsd:double.
	
#defintion jt:highestRatingTime
jt:highestRatingTime rdfs:domain foaf:Person;
	rdfs:range xsd:integer.

#defintion of inferred knowledge
jt:PopularFilm rdfs:SubClassOf dbo:Film.
jt:UnpopularFilm rdfs:SubClassOf dbo:Film.
jt:PopularFilm owl:disjointWith jt:UnpopularFilm.