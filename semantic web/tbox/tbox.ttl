@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dbc: <http://dbpedia.org/resource/Category:> . 
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://dublincore.org/documents/2012/06/14/dcmi-terms/> .
@prefix schema: <https://schema.org/place> .
@prefix jr: <http://schema.example.org/> .
@prefix xsd: <https://www.w3.org/2011/rdf-wg/wiki/XSD_Datatypes> .


# definition of hasRated property
jt:hasRated	a rdf:Property.
jt:hasRated rdfs:subPropertyOf foaf:topic_interest.
jt:hasRated rdfs:domain foaf:Person.
jt:hasRated rdfs:range dbo:Film.

#definition of ratedBy property which is inverse of hasRated
jt:ratedBy a rdf:Property.
jt:ratedBy owl:inverseOf :hasRated.

#definition of total rating literal
jt:totalRatings a rdfs:Literal.
jt:totalRatings rdfs:range xsd:nonNegativeInteger.

#definition of number of ratings property 
jt:numberOfRatings a rdf:Property.
jt:numberOfRatings rdfs:domain dbo:Film.
jt:numberOfRatings rdfs:range jt:totalRatings.

#definition of avg rating literal
jt:avgOfRatings a rdfs:Literal.
jt:avgOfRatings rdfs:range xsd:double.

#definition of avg of ratings property 
jt:avgRating a rdf:Property.
jt:avgRating rdfs:domain dbo:Film.
jt:avgRating rdfs:range jt:avgOfRatings.

#
jt:favoriteGenre a rdf:property.
jt:favoriteGenre rdfs:domain foaf:Person.
jt:favoriteGenre rdfs:range dbo:Genre.



#defintion of inferred knowledge
jt:PopularFilm rdfs:SubClassOf dbo:Film.
jt:UnpopularFilm rdfs:SubClassOf dbo:Film.
jt:PopularFilm owl:disjointWith jt:UnpopularFilm.